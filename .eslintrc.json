{
    "extends": "jquery",
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jquery": true
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-undef": ["off"],
        "linebreak-style": ["off"],
        "space-in-parens": ["error", "never"],
        "array-bracket-spacing": ["off"],
        "lines-around-comment": ["off"],
        "eol-last": ["off"],
        "quotes": ["error", "single"],
        "no-multiple-empty-lines": [1, { "max": 1 }],
        "max-len": "off",
        "max-statements-per-line": ["error", { "max": 1 }],
        "block-scoped-var": 2,
        "dot-notation": 2,
        "eqeqeq": 2,
        "semi": 2,
        "no-else-return": 2,
        "no-fallthrough": 2,
        "no-redeclare": 2,
        "wrap-iife": 2,
        "yoda": 0
    }
}